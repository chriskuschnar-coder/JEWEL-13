{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:production": "npm run build",
    "start": "node server.js",
    "postbuild": "echo 'Build completed successfully'",
    "load:smoke": "k6 run load/k6-smoke.js",
    "load:stress": "k6 run --vus 50 --duration 5m load/k6-smoke.js",
    "load:spike": "k6 run --vus 100 --duration 1m load/k6-smoke.js",
    "load:soak": "k6 run --vus 20 --duration 30m load/k6-smoke.js"
  },
